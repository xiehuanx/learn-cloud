<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.icedevcloud.learn.mapper.QuestionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.icedevcloud.learn.entity.Question">
        <id column="id" property="id" />
        <result column="question_type" property="questionType" />
        <result column="curriculum_id" property="curriculumId" />
        <result column="correct_answer" property="correctAnswer" />
        <result column="wrong_answer" property="wrongAnswer" />
        <result column="question" property="question" />
        <result column="gmt_create" property="gmtCreate" />
        <result column="gmt_modified" property="gmtModified" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, question_type, curriculum_id, correct_answer, wrong_answer, question, gmt_create, gmt_modified
    </sql>

</mapper>
